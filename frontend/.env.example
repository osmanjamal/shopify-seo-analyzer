# Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_WS_URL=ws://localhost:5000

# Google OAuth Configuration
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here
REACT_APP_GOOGLE_ANALYTICS_VIEW_ID=your_analytics_view_id

# Google APIs
REACT_APP_GOOGLE_MAPS_API_KEY=your_maps_api_key_here

# Application Settings
REACT_APP_APP_NAME=Shopify SEO Analyzer
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_REALTIME=true
REACT_APP_ENABLE_COMPETITOR_ANALYSIS=true
REACT_APP_ENABLE_AI_INSIGHTS=false

# Debug Settings
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info

# External Services
REACT_APP_SENTRY_DSN=
REACT_APP_INTERCOM_APP_ID=
REACT_APP_HOTJAR_ID=

# Security
REACT_APP_ENABLE_HTTPS=false
REACT_APP_CSP_REPORT_URI=

# Limits
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_MAX_KEYWORDS=1000
REACT_APP_MAX_WEBSITES=10

# Cache Settings (in seconds)
REACT_APP_CACHE_TTL_SHORT=300
REACT_APP_CACHE_TTL_MEDIUM=3600
REACT_APP_CACHE_TTL_LONG=86400